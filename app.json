{
  "expo": {
    "name": "Pin Tracker",
    "slug": "pintracker_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matthewversteeg.pintracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.pintracker"
    },
    "web": {
      "favicon": "./assets/icon.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "d70c2914-7382-4bd3-a5d7-d45bd5f3195f"
      }
    }
  }
}
